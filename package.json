{
  "name": "@aioutlet/e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end integration tests for AIOutlet microservices platform",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest tests/auth",
    "test:user": "jest tests/user",
    "test:workflow": "jest tests/workflows",
    "services:up": "docker-compose up -d",
    "services:down": "docker-compose down",
    "services:logs": "docker-compose logs -f",
    "services:status": "docker-compose ps",
    "wait-for-services": "node scripts/wait-for-services.js",
    "pretest": "npm run wait-for-services"
  },
  "keywords": [
    "e2e",
    "integration",
    "testing",
    "microservices",
    "aioutlet"
  ],
  "author": "AIOutlet Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-jest": "^30.0.0",
    "jest": "^30.0.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
